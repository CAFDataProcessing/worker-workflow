<workflow>
    <details>
        <name>Custom Data Test</name>
        <description></description>
        <notes></notes>
        <id>4</id>
    </details>
    <processingRules>
        <processingRule>
            <details>
                <name>Custom data test</name>
                <description></description>
                <id>5</id>
                <priority>1</priority>
                <enabled>true</enabled>
            </details>
            <actions>
                <action>
                    <details>
                        <settings class="linked-hash-map">
                            <workerName>testworker</workerName>
                            <customData class="linked-hash-map">
                                <test_prop class="linked-hash-map">
                                    <source>inlineJson</source>
                                    <data class="linked-hash-map">
                                        <myObject class="linked-hash-map">
                                            <myKey> myValue</myKey>
                                            <myArray class="list">
                                                <linked-hash-map>
                                                    <myNullValue class="null"/>
                                                </linked-hash-map>
                                                <linked-hash-map>
                                                    <myStringValue>My String</myStringValue>
                                                </linked-hash-map>
                                                <linked-hash-map>
                                                    <myBoolean class="boolean">true</myBoolean>
                                                </linked-hash-map>
                                                <linked-hash-map>
                                                    <myInteger class="int">1</myInteger>
                                                </linked-hash-map>
                                            </myArray>
                                            <intArray class="list">
                                                <int class="int">1</int>
                                                <int class="int">2</int>
                                                <int class="int">3</int>
                                                <int class="int">4</int>
                                            </intArray>
                                        </myObject>
                                    </data>
                                </test_prop>
                                <test_projectId_prop class="linked-hash-map">
                                    <source>projectId</source>
                                </test_projectId_prop>
                                <invalid_prop>
                                    <source>nonValidSource</source>
                                    <data>invalid</data>
                                </invalid_prop>
                                <another_prop>second value</another_prop>
                            </customData>
                        </settings>
                        <name>String condition action</name>
                        <description></description>
                        <typeInternalName>CompositeDocumentWorkerHandler</typeInternalName>
                        <typeId>12</typeId>
                        <id>10</id>
                        <order>100</order>
                    </details>
                    <conditions/>
                </action>
            </actions>
            <conditions/>
        </processingRule>
        <processingRule>
            <details>
                <name>Not enabled rule</name>
                <description></description>
                <id>6</id>
                <priority>2</priority>
                <enabled>false</enabled>
            </details>
            <actions>
                <action>
                    <details>
                        <settings class="linked-hash-map">
                            <workerName>entityworker</workerName>
                            <customData class="linked-hash-map">
                                <OPERATION_MODE>DETECT</OPERATION_MODE>
                                <GRAMMAR_MAP>{pii.xml: []}</GRAMMAR_MAP>
                            </customData>
                        </settings>
                        <name>Entity action</name>
                        <description></description>
                        <typeInternalName>CompositeDocumentWorkerHandler</typeInternalName>
                        <typeId>12</typeId>
                        <id>11</id>
                        <order>100</order>
                    </details>
                    <conditions/>
                </action>
            </actions>
            <conditions/>
        </processingRule>
        <processingRule>
            <details>
                <name>Another enabled rule</name>
                <description></description>
                <id>7</id>
                <priority>3</priority>
                <enabled>true</enabled>
            </details>
            <actions>
                <action>
                    <details>
                        <settings class="linked-hash-map">
                            <workerName>dummyworker</workerName>
                            <customData class="linked-hash-map">
                                <dummyField>dummyValue</dummyField>
                            </customData>
                        </settings>
                        <name>Dummy</name>
                        <description></description>
                        <typeInternalName>CompositeDocumentWorkerHandler</typeInternalName>
                        <typeId>12</typeId>
                        <id>12</id>
                        <order>100</order>
                    </details>
                    <conditions/>
                </action>
            </actions>
            <conditions/>
        </processingRule>
    </processingRules>
</workflow>